// .vscode/launch.json
{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Start Frontend",
        "preLaunchTask": "Start Frontend",  // Aqui referenciamos o task "npm start"
        "skipFiles": [
          "<node_internals>/**"
        ],
        "program": "${workspaceFolder}/src/index.tsx",  // Caminho do ponto de entrada da aplicação
        "outFiles": [
          "${workspaceFolder}/**/*.js"
        ]
      }
    ]
  }
  